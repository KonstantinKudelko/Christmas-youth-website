{
  "kind": "collectionType",
  "connection": "default",
  "collectionName": "articles",
  "info": {
    "name": "Article"
  },
  "options": {
    "increments": true,
    "timestamps": true
  },
  "attributes": {
    "title": {
      "type": "string"
    },
    "Body": {
      "type": "richtext"
    },
    "cover": {
      "model": "file",
      "via": "related",
      "plugin": "upload",
      "required": false
    },
    "tags": {
      "collection": "tag",
      "via": "articles"
    },
    "author": {
      "model": "authors",
      "via": "articles"
    }
  }
}
